{
  "name": "planning-poker",
  "private": true,
  "version": "0.0.0",
  "homepage": "/",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage --coverage.thresholds=false",
    "test:ui": "vitest --ui",
    "coverage:report": "vitest run --coverage --coverage.thresholds=false --reporter=html",
    "docs": "jsdoc -c jsdoc.json src/components src/pages src/stores src/utils",
    "docs:serve": "npx http-server docs -p 3003",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "ci:test": "vitest run --coverage.enabled=true --coverage.reporter=json-summary --coverage.reporter=json",
    "ci:build": "vite build --mode production"
  },
  "dependencies": {
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "socket.io-client": "^4.8.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdoc": "^4.0.5",
    "jsdom": "^27.3.0",
    "minami": "^1.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "vite": "^7.2.4",
    "vitest": "^4.0.15"
  }
}
